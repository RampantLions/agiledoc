<?xml version="1.0" encoding="UTF-8"?>
<project>
	<projectName>Jogo Da Velha</projectName>
	<projectDomain>jogoDaVelha</projectDomain>
	<projectDescription>O Jogo da Velha (portugues brasileiro) ou jogo do
		galo (portugues europeu) e um jogo e passatempo popular. Um jogo de
		lapis e papel para dois jogadores, O e X, que se revezam marcando os
		espacos em uma grade 3x3. O jogador que conseguir colocar tres
		respectivas marcas em uma linha horizontal, vertical ou diagonal,
		ganha o jogo.</projectDescription>
	<projectComponents>
		<projectComponent componentName="Identificacao do Jogador"
			componentPath="identificacaoDoJogador/client">
			<entries>
				<entry featureType="">
					<className>EntradaNoJogo</className>
					<classPath>identificacaoDoJogador/client</classPath>
					<featurePath></featurePath>
					<featureName>Entrada No Jogo</featureName>
					<featureDescription>Formulario para identificacao do nome e da
						senha do jogador.</featureDescription>
					<methods />
				</entry>
				<entry featureType="">
					<className>SaidaDoJogo</className>
					<classPath>identificacaoDoJogador/client</classPath>
					<featurePath></featurePath>
					<featureName>Saida Do Jogo</featureName>
					<featureDescription>Link para saida segura do jogo.
					</featureDescription>
					<methods />
				</entry>
			</entries>
		</projectComponent>
		<projectComponent componentName="Infra-Estrutura"
			componentPath="server">
			<entries>
				<entry featureType="architecture">
					<className>EntradaParaAsImplementacoesDoServidor</className>
					<classPath>server</classPath>
					<featurePath></featurePath>
					<featureName>Entrada Para As Implementacoes Do Servidor
					</featureName>
					<featureDescription>The server side implementation of the RPC
						service.</featureDescription>
					<methods>
						<method>
							<methodName>checkForWinner</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="">
					<className>JulgamentoDoMovimento</className>
					<classPath>server</classPath>
					<featurePath></featurePath>
					<featureName>Julgamento Do Movimento</featureName>
					<featureDescription>Verifica se o movimento do jogador executou uma
						sequencia ganhadora ou nao.</featureDescription>
					<methods>
						<method>
							<methodName>checkForWinner</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>checkWinnerRow</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>checkWinnerColumn</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>checkWinnerDiagonalRight</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>checkWinnerDiagonalLeft</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
			</entries>
		</projectComponent>
		<projectComponent componentName="Jogo" componentPath="client">
			<entries>
				<entry featureType="architecture">
					<className>JogoDaVelha</className>
					<classPath>client</classPath>
					<featurePath></featurePath>
					<featureName>Jogo Da Velha</featureName>
					<featureDescription>Classe ancora para inicializacao do sistema
						pelo Google Web Toolkit
						(EntryPoint).</featureDescription>
					<methods>
						<method>
							<methodName>onModuleLoad</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="architecture">
					<className>ChamadaParaOJulgamentoDoMovimento</className>
					<classPath>client/arquitetura</classPath>
					<featurePath>arquitetura</featurePath>
					<featureName>Chamada Para O Julgamento Do Movimento</featureName>
					<featureDescription>Create a remote service proxy to talk to the
						server-side Greeting service.</featureDescription>
					<methods>
						<method>
							<methodName>checkForWinner</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="architecture">
					<className>InicializacaoDaAjuda</className>
					<classPath>client/arquitetura</classPath>
					<featurePath>arquitetura</featurePath>
					<featureName>Inicializacao Da Ajuda</featureName>
					<featureDescription>Carregamento do conteudo da ajuda do sistema.
					</featureDescription>
					<methods />
				</entry>
				<entry featureType="architecture">
					<className>InicializacaoDoJogo</className>
					<classPath>client/arquitetura</classPath>
					<featurePath>arquitetura</featurePath>
					<featureName>Inicializacao Do Jogo</featureName>
					<featureDescription>Preparacao do sistema para a inicia��o de um
						jogo.</featureDescription>
					<methods />
				</entry>
				<entry featureType="architecture">
					<className>InterfaceComOServidorDaInfraEstrutura</className>
					<classPath>client/arquitetura</classPath>
					<featurePath>arquitetura</featurePath>
					<featureName>Interface Com O Servidor Da Infra Estrutura
					</featureName>
					<featureDescription>The client side stub for the RPC service.
					</featureDescription>
					<methods>
						<method>
							<methodName>checkForWinner</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="architecture">
					<className>InterfaceComOServidorDaInfraEstruturaAsync</className>
					<classPath>client/arquitetura</classPath>
					<featurePath>arquitetura</featurePath>
					<featureName>Interface Com O Servidor Da Infra Estrutura Async
					</featureName>
					<featureDescription>Interface Ass�ncrona para o servidor de
						Infra-estrutura do sistema.</featureDescription>
					<methods>
						<method>
							<methodName>checkForWinner</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="">
					<className>GradeDoJogo</className>
					<classPath>client/interfaceComOUsuario</classPath>
					<featurePath>interfaceComOUsuario</featurePath>
					<featureName>Grade Do Jogo</featureName>
					<featureDescription>3x3 grid of cells in which players enter a mark
						to indicate their desired
						move.</featureDescription>
					<methods>
						<method>
							<methodName>makeMove</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>formatGridCells</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="feature">
					<className>MovimentoDoJogador</className>
					<classPath>client/interfaceComOUsuario</classPath>
					<featurePath>interfaceComOUsuario</featurePath>
					<featureName>Movimento Do Jogador</featureName>
					<featureDescription>Quando o jogador clica numa celula na grade do
						tabuleiro o jogo desenha um 0 ou X
						dependendo de qual e o jogador atual.</featureDescription>
					<methods>
						<method>
							<methodName>makeMove</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>cellIsEmpty</methodName>
							<methodDescription>The Player can select a cell only if it wasn't
								already selected.</methodDescription>
						</method>
						<method>
							<methodName>toggleCurrentPlayer</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="feature">
					<className>SequenciaGanhadora</className>
					<classPath>client/interfaceComOUsuario</classPath>
					<featurePath>interfaceComOUsuario</featurePath>
					<featureName>Sequencia Ganhadora</featureName>
					<featureDescription>No aparecimento de 3 simbolos iguais
						consecutivos no tabuleiro (horizontal,
						vertical ou diagonal), o jogador corrente vence o jogo e o sistema exibe a
						sequencia ganhadora em vermelho.</featureDescription>
					<methods>
						<method>
							<methodName>showWinner</methodName>
							<methodDescription></methodDescription>
						</method>
						<method>
							<methodName>showHelpHint</methodName>
							<methodDescription></methodDescription>
						</method>
					</methods>
				</entry>
				<entry featureType="">
					<className>Tabuleiro</className>
					<classPath>client/interfaceComOUsuario</classPath>
					<featurePath>interfaceComOUsuario</featurePath>
					<featureName>Tabuleiro</featureName>
					<featureDescription>The web page where the game is shown.
						Accommodates the game grid and also
						some options such as a restart button, help, game score, game status,
						...</featureDescription>
					<methods />
				</entry>
			</entries>
		</projectComponent>
	</projectComponents>
</project>
