<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  	<servlet>
		<servlet-name>projectDatabaseAccess</servlet-name>
		<servlet-class>sourceagile.server.databaseAccess.Project.ProjectDatabaseAccessImplementation</servlet-class>
	</servlet>
	
		<servlet>
		<servlet-name>subversionRepositoryClassLoader</servlet-name>
		<servlet-class>sourceagile.server.classRepositories.subversionClassRepository.SubversionRepositoryClassImplementation</servlet-class>
	</servlet>
	
		<servlet-mapping>
		<servlet-name>projectDatabaseAccess</servlet-name>
		<url-pattern>/sourceagileappengine/projectDatabaseAccess</url-pattern>
	</servlet-mapping>
	
		<servlet-mapping>
		<servlet-name>subversionRepositoryClassLoader</servlet-name>
		<url-pattern>/sourceagileappengine/subversionRepositoryClassLoader</url-pattern>
	</servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>SourceAgileAppEngine.html</welcome-file>
  </welcome-file-list>

</web-app>
